{
  "name": "allforms",
  "version": "0.1.0",
  "description": "a form building app",
  "main": "dist/main.js",
  "scripts": {
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "pretest": "del-cli \"test/coverage\"",
    "test": "cross-env BABEL_ENV=webpack NODE_ENV=test ./node_modules/.bin/karma start test/karma.conf.js",
    "test:debug": "npm test -- --single-run=false --debug",
    "clean": "npm cache clean && del-cli node_modules \"test/coverage\" dist",
    "clean:dist": "del-cli dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "cross-env BABEL_ENV=webpack NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "cross-env BABEL_ENV=webpack NODE_ENV=production npm run webpack -- --progress --profile",
    "start": "npm-run-all --parallel lint:watch start-mockapi server:dev",
    "secure": "nsp check",
    "server": "npm run server:dev",
    "server:dev": "cross-env BABEL_ENV=webpack NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "webpack": "cross-env BABEL_ENV=webpack ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack-dev-server",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "cross-env BABEL_ENV=node babel-node ./build/server.js",
    "generate-mock-data": "cross-env BABEL_ENV=node babel-node ./build/generate-mockdata"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmzagorski/allforms.git"
  },
  "keywords": [
    "aurelia",
    "allforms",
    "forms",
    "building"
  ],
  "license": "CC0-1.0",
  "author": "Jeremy Zagorski <jmzagorski@gmail.com>",
  "bugs": {
    "url": "https://github.com/jmzagorski/allforms/issues"
  },
  "homepage": "https://github.com/jmzagorski/allforms#readme",
  "aurelia": {
    "build": {
      "resources": []
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.1.0",
    "aurelia-dialog": "^1.0.0-beta.3.0.1",
    "aurelia-event-aggregator": "^1.0.0",
    "aurelia-fetch-client": "^1.0.1",
    "aurelia-framework": "^1.0.7",
    "aurelia-history-browser": "^1.0.0",
    "aurelia-http-logger": "^2.0.0",
    "aurelia-loader-webpack": "^1.0.3",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal": "^1.2.0",
    "aurelia-pal-browser": "^1.0.0",
    "aurelia-polyfills": "^1.1.1",
    "aurelia-redux-plugin": "0.1.1",
    "aurelia-route-recognizer": "^1.1.0",
    "aurelia-router": "^1.0.7",
    "aurelia-templating-binding": "^1.1.0",
    "aurelia-templating-resources": "^1.2.0",
    "aurelia-templating-router": "^1.0.0",
    "aurelia-value-converters": "^1.0.2",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.4.6",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "hot-formula-parser": "^2.0.1",
    "interact.js": "^1.2.8",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-textcomplete": "^1.8.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.1",
    "slickgrid-es6": "^2.0.3",
    "stampit": "^3.1.2"
  },
  "devDependencies": {
    "aurelia-testing": "^1.0.0-beta.2.0.1",
    "aurelia-tools": "^1.0.0",
    "aurelia-webpack-plugin": "^1.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.0.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.12.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "faker": "^4.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-minifier": "^3.2.3",
    "html-minifier-loader": "^1.3.4",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "jasmine-data-provider": "^2.2.0",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-remap-istanbul": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "multiparty": "^4.1.3",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "postcss-loader": "^1.3.3",
    "randexp": "^0.4.5",
    "raw-loader": "^0.5.0",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "wait-on": "^2.0.1",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0"
  }
}
